# {{ ansible_managed }}

{% if 'path' in item %}
{{ item.path -}}
{% endif %}
 {
  {% if item.options is defined -%}
  {% for option in item.options -%}
  {{ option }}
  {% endfor -%}
  {% endif %}
  {%- if item.scripts is defined -%}
  {%- for script in item.scripts -%}
{
    {{ script }}
  }
  {% endfor -%}
  {% endif -%}
}
